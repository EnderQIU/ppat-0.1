>>> from big_phoney import PredictionModel, PhoneticDictionary

>>> pred_model    = PredictionModel()
>>> phonetic_dict = PhoneticDictionary()

>>> pred_model.predict('Alexandria')
'AE2 L IH0 G Z AE1 N D R IY0 AH0'
>>> phonetic_dict.lookup('Alexandria')
'AE2 L AH0 G Z AE1 N D R IY0 AH0'

>>> phonetic_dict.lookup('Augusta')
'AH0 G AH1 S T AH0'
>>> pred_model.predict('Augusta')
'OW0 G UW1 S T AH0'

